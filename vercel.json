{
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "redirects": [
        {
            "source": "/images/logo",
            "destination": "/logo-og.jpg"
        },
        {
            "source": "/images/icon",
            "destination": "/logo.svg"
        },
        {
            "source": "/(\\w+|\\W+|\/+)",
            "destination": "/"
        }
    ]
}