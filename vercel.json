{
    "functions": {
        "api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "redirects": [
        {
            "source": "/images/logo",
            "destination": "/logo.png"
        },
        {
            "source": "/(\\w+|\\W+|\/+)",
            "destination": "/"
        }
    ]
}